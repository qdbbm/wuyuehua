/* pages/coupon/coupon.wxss */
page {
  background: #f9f9f9;
}

page:after {
  position: absolute;
  bottom: 0;
  right: 0;
  z-index: -1;
  width: 395rpx;
  height: 337rpx;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 100%;
  content: "";
}

.coupon-item {
  position: relative;
  border-radius: 15rpx;
}

.coupon-item:after {
  position: absolute;
  top: 0;
  right: 10rpx;
  width: 124rpx;
  height: 62rpx;
  border-radius: 0 0 62rpx 62rpx;
  content: "";
}

.coupon-item:not(:first-child) {
  margin-top: 30rpx;
}

.coupon-item__num {
  position: relative;
  width: 200rpx;
  font-family: "arial";
  font-size: 45rpx;
  font-weight: 700;
  color: #fff;
}

.coupon-item__num:after {
  position: absolute;
  top: 10rpx;
  left: -2rpx;
  bottom: 10rpx;
  width: 7rpx;
  background: url("data:image/png;base64,") repeat-y;
  background-size: 100%;
  content: "";
}

.coupon-item__num .num {
  font-size: 88rpx;
}

.coupon-item__bd {
  position: relative;
  padding: 40rpx 25rpx;
}

.coupon-item__bd:before,
.coupon-item__bd:after {
  position: absolute;
  right: -13rpx;
  z-index: 2;
  width: 24rpx;
  height: 15rpx;
  background: #f9f9f9;
  border: 1rpx solid #f1d1a4;
  content: "";
}

.coupon-item__bd:before {
  top: -1rpx;
  border-top: 0;
  border-bottom-right-radius: 24rpx;
  border-bottom-left-radius: 24rpx;
}

.coupon-item__bd:after {
  bottom: -1rpx;
  border-top-right-radius: 24rpx;
  border-top-left-radius: 24rpx;
  border-bottom: 0;
}

.coupon-item__status {
  padding: 0 10rpx;
  height: 36rpx;
  line-height: 36rpx;
  border-radius: 8rpx;
  color: #fff;
}

.coupon-item__btn {
  position: relative;
  z-index: 1;
  width: 100rpx;
  padding: 0 30rpx;
  text-align: center;
  overflow: hidden;
  box-sizing: border-box;
}

.coupon-item__btn.disabled {
  color: #e5e5e5 !important;
  pointer-events: none;
}

.coupon-item--djq {
  border: 1rpx solid #f1d1a4;
  border-left: 20rpx solid #bfa179;
}

.coupon-item--djq:after {
  background: #fff4e5;
}

.coupon-item--djq .coupon-item__num {
  background: #f1d1a4;
}

.coupon-item--djq .coupon-item__status {
  background: #bfa179;
}

.coupon-item--djq .coupon-item__txt,
.coupon-item--djq .coupon-item__time {
  color: #bfa179;
}

.coupon-item--djq .coupon-item__btn {
  color: #c7a16d;
  border-left: 1rpx dashed #f1d1a4;
}

.coupon-item--zkq {
  border: 1rpx solid #b9b0a3;
  border-left: 20rpx solid #9c907f;
}

.coupon-item--zkq:after {
  background: #f5f5f2;
}

.coupon-item--zkq .coupon-item__num {
  background: #b9b0a3;
}

.coupon-item--zkq .coupon-item__status {
  background: #b9b0a3;
}

.coupon-item--zkq .coupon-item__txt,
.coupon-item--zkq .coupon-item__time {
  color: #bbb7b2;
}

.coupon-item--zkq .coupon-item__btn {
  color: #b9b0a3;
  border-left: 1rpx dashed #beb6aa;
}

.coupon-item--zkq .coupon-item__bd:before,
.coupon-item--zkq .coupon-item__bd:after {
  border-color: #b9b0a3;
}

.coupon-item--disabled {
  border: 1rpx solid #e5e5e5;
  border-left: 20rpx solid #cbcbcb;
}

.coupon-item--disabled .coupon-item__num {
  background: #e5e5e5;
}

.coupon-item--disabled .coupon-item__status {
  background: #e5e5e5;
}

.coupon-item--disabled .coupon-item__txt,
.coupon-item--disabled .coupon-item__time {
  color: #bbb7b2;
}

.coupon-item--disabled .coupon-item__btn {
  color: #e5e5e5;
  border-left: 1rpx dashed #e5e5e5;
}

.coupon-item--disabled .coupon-item__bd:before,
.coupon-item--disabled .coupon-item__bd:after {
  border-color: #e5e5e5;
}

.coupon-item--disabled .coupon-item__bd-tle {
  color: #999;
}

.coupon-item--sm .coupon-item__num {
  width: 160rpx;
}

.coupon-item--sm .coupon-item__num .num {
  font-size: 70rpx;
}

.coupon-item__checked {
  position: absolute;
  top: 50%;
  left: -70rpx;
  width: 34rpx;
  height: 34rpx;
  background: #eee;
  border-radius: 50%;
  border: 1rpx solid #c1c1c1;
  transform: translate(0, -50%);
  box-sizing: border-box;
}

.coupon-item--active .coupon-item__checked {
  background: #fff;
  border: 9rpx solid #ff5d16;
}