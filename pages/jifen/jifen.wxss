/* pages/jifen/jifen.wxss */
.jifen-hd {
  height: 360rpx;
  box-sizing: border-box;
  background: url("data:image/jpeg;base64,") no-repeat;
  background-size: 100%;
}

.jifen-hd__tle {
  display: inline-block;
  height: 50rpx;
  line-height: 50rpx;
  padding: 0 20rpx;
  background: linear-gradient(to right, #fef3dc, #ffecb8);
  border-top-left-radius: 20rpx;
  border-top-right-radius: 25rpx;
  border-bottom-right-radius: 25rpx;
}

.jf-detail-list > view {
  border-bottom: 1rpx dashed #e3e3e3;
}